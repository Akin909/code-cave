{
  "name": "React-Boilerplate",
  "version": "1.0.0",
  "description": "A Boilerplate setup for new react projects",
  "main": "index.js",
  "author": "Akin Sowemimo",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "server": "nodemon ./../server/server.js --exec babel-node -e js",
    "both": "concurrently \"webpack-dev-server\" \"nodemon ./../server/server.js --exec babel-node -e js\"",
    "build": "webpack -p"
  },
  "devDependencies": {
